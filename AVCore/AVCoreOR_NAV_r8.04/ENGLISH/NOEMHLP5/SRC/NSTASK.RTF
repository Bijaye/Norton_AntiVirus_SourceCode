\par \pard\plain \s2\li115\ri130\sb120\sa120\widctlpar\tx432 \b\f5\fs20 \page  {\cs20\super #{\footnote \pard\plain \li115\ri130\sb60\sa120\widctlpar\tx432 \f5\fs20 {\cs20\super #} IDH_NAVW_PROC_INITIATE_SCANS}} {\cs20\super ${\footnote \pard\plain 
