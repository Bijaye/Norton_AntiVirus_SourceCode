[APACHE]
title=NU 9.0

Map=S:,\\USSM-DEV2\VOL2\APACHE\SOURCE
Map=I:,\\USSM-DEV2\VOL2\APACHE\SOURCE\INCLUDE\SRC%PROJVERS%
