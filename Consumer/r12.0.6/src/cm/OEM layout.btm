rem /////////////////////////////////////////////////////////////
rem Get the path to the batch file and use as the root for all
rem other relative paths
rem
	set root=%@path[%_batchname%]%
	echo ROOT = %root%
	rem If the last character in root is not a '\', then append one.
	iff %@SUBSTR[%root%,%@EVAL[%@LEN[%root%]-1],1] NE \ then 
 	   set root=%root%\
	endiff

md %root%..\Final
set ALL=1
set AVSTE_DLL =
set NAVOPTRF_DLL =
set AVRES_DLL =
set BRANDING_INI =
set HTML_HELP =
set WIN_HELP =
set QCONSOLE_HELP =
set INSTALL_TXT=0
set Localization=0
set SCSS=0
set NOSB=0
set AUTORUN=0
set CDSTART=0
set IWP=1
set EXTERNAL_FILES=
set MULTI_PK=0
set BASE_INSTOPTS=%@FULL[..]\Nobuilds\configfiles\Instopts_Base.dat
set LICENSE_INSTOPTS=
set SKU_DAT=
set CUSTOM_INSTOPTS=
set TOOLS_FOLDER=%root%tools\
set CTO=0

rem -----------------------------
rem check for known parameters
rem -----------------------------
do param = 1 to 127
  shift 1
  iff "%0"=="" then
     shift -%param%
     goto continue
  endiff
  iff "%@upper[%0]"=="/PREPAID" then
  	 set PREPAID=/PREPAID
  	 set ALL=0
  endiff
  iff "%@upper[%0]"=="/OEM" then
  	 set OEMLAYOUT=/OEM
  	 set ALL=0
  endiff
enddo
shift -127

:continue

