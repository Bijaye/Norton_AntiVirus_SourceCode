	set LAYOUT_NAME=%@REPLACE[VERSION,11,%NAME%]%
	rem Make the folders needed for all layouts
	md /s %LAYOUT_DEST%%LAYOUT_NAME%
	md %LAYOUT_DEST%%LAYOUT_NAME%\NAV
